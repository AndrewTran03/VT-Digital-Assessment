{
  "version": 2,
  "builds": [
    {
      "src": "server/dist/src/backend.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/dist/**"]
      }
    }
  ],
  "routes": [
    { 
      "src": "/server/src/(.*)", 
      "dest": "server/dist/src/backend.js" 
    }
  ]
}
